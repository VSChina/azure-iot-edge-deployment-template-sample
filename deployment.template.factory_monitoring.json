{
    "$schema-template": "2.0.0",
    "modulesContent": {
      "$edgeAgent": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "runtime": {
            "type": "docker",
            "settings": {
              "minDockerVersion": "v1.25",
              "loggingOptions": "",
              "registryCredentials": {
                "my_acr1": {
                  "username": "${ACR_USER_1}",
                  "password": "${ACR_PASSWORD_1}",
                  "address": "${ACR_ADDRESS_1}"
                },
                "my_acr2": {
                    "username": "${ACR_USER_2}",
                    "password": "${ACR_PASSWORD_2}",
                    "address": "${ACR_ADDRESS_2}"
                  }
              }
            }
          },
          "systemModules": {
            "edgeAgent": {
              "type": "docker",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
                "createOptions": {}
              }
            },
            "edgeHub": {
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
                "createOptions": {
                  "HostConfig": {
                    "PortBindings": {
                      "5671/tcp": [
                        {
                          "HostPort": "5671"
                        }
                      ],
                      "8883/tcp": [
                        {
                          "HostPort": "8883"
                        }
                      ],
                      "443/tcp": [
                        {
                          "HostPort": "443"
                        }
                      ]
                    }
                  }
                }
              }
            }
          },
          "modules": {
            "tempSensor": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "mcr.microsoft.com/azureiotedge-simulated-temperature-sensor:1.0",
                "createOptions": {}
              }
            },
            "HumidityFilterModule": {
              "version": "1.0",
              "type": "docker",
              "status": "running",
              "restartPolicy": "always",
              "settings": {
                "image": "${ACR_ADDRESS_1}/humidityfiltermodule:0.0.19-amd64",
                "createOptions": {
                    "threshold": 25
                }
              }
            },
            "TemperatureFilterModule": {
                "version": "1.0",
                "type": "docker",
                "status": "running",
                "restartPolicy": "always",
                "settings": {
                  "image": "${ACR_ADDRESS_2}/temperaturefiltermodule:0.0.18-amd64",
                  "createOptions": {
                    "threshold": 25
                  }
                }
              }
          }
        }
      },
      "$edgeHub": {
        "properties.desired": {
          "schemaVersion": "1.0",
          "routes": {
            "sensorToHumidityFilterModule": "FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/HumidityFilterModule/inputs/input1\")",
            "HumidityFilterModuleToIoTHub": "FROM /messages/modules/HumidityFilterModule/outputs/* INTO $upstream",
            "sensorToTemperatureFilterModule": "FROM /messages/modules/tempSensor/outputs/temperatureOutput INTO BrokeredEndpoint(\"/modules/TemperatureFilterModule/inputs/input1\")",
            "TemperatureFilterModuleToIoTHub": "FROM /messages/modules/TemperatureFilterModule/outputs/* INTO $upstream"
          },
          "storeAndForwardConfiguration": {
            "timeToLiveSecs": 7200
          }
        }
      }
    }
  }
